# Flogr Environment Configuration
# Copy this file to .env and fill in your values

# Flickr API Key (Required)
# Get your API key from: https://www.flickr.com/services/api/misc.api_keys.html
FLICKR_API_KEY=your_flickr_api_key_here

# Map feature now uses OpenStreetMap - No API key needed!

# Optional: MySQL Cache Configuration
CACHE_SQL_USER=
CACHE_SQL_PASSWORD=
CACHE_SQL_SERVER=
CACHE_SQL_DATABASE=

# Optional: File System Cache Path
CACHE_PATH=
